<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".profissional.gui.HomeProfissional">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="email@emal.com"
        android:textColor="#DD000000"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="616dp" />

    <TextView
        android:id="@+id/lblCPF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CPF:"
        android:textColor="#000000"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="586dp" />

    <TextView
        android:id="@+id/lblEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email:"
        android:textColor="#000000"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="616dp" />

    <Button
        android:id="@+id/botao_att_perfil"
        android:layout_width="341dp"
        android:layout_height="53dp"
        android:background="#AF0D25"
        android:text="Atualizar Perfil"
        android:textColor="#FFFFFF"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="662dp" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/barra_superior2"
        android:layout_width="411dp"
        android:layout_height="wrap_content"
        android:background="#AF0D25"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        tools:ignore="MissingConstraints" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/image"
        android:layout_width="101dp"
        android:layout_height="74dp"
        android:src="@mipmap/ic_launcher"
        tools:layout_editor_absoluteX="155dp"
        tools:layout_editor_absoluteY="71dp" />

    <TextView
        android:id="@+id/nomeprofissional"
        android:layout_width="302dp"
        android:layout_height="65dp"
        android:layout_centerVertical="true"
        android:layout_toEndOf="@+id/image"
        android:text="@string/nome_do_profissional"
        android:textAlignment="center"
        android:textColor="#000"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="167dp" />

    <TextView
        android:id="@+id/decricaoprofissional"
        android:layout_width="398dp"
        android:layout_height="226dp"
        android:background="#80CACACA"
        android:text="Descrição do profissional"
        android:textColor="#000000"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="253dp" />

    <TextView
        android:id="@+id/lblVisitas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Visitas:"
        android:textColor="#000000"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="489dp" />

    <TextView
        android:id="@+id/telefone_profissional"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Telefone:"
        android:textColor="#000000"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="519dp" />

    <TextView
        android:id="@+id/lblEndereco"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Endereço:"
        android:textColor="#000000"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="552dp" />

    <TextView
        android:id="@+id/qtt_visitas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:textColor="#DD000000"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="73dp"
        tools:layout_editor_absoluteY="489dp" />

    <TextView
        android:id="@+id/qtt_visitas2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="000.000.000-00"
        android:textColor="#DD000000"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="52dp"
        tools:layout_editor_absoluteY="586dp" />

    <TextView
        android:id="@+id/endereco"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rua exemplo"
        android:textColor="#DD000000"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="91dp"
        tools:layout_editor_absoluteY="552dp" />

    <TextView
        android:id="@+id/telefone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="(00) 90000-0000"
        android:textColor="#DD000000"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="82dp"
        tools:layout_editor_absoluteY="519dp" />

</androidx.constraintlayout.widget.ConstraintLayout>