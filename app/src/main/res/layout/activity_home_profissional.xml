<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".profissional.gui.HomeProfissional">

    <Button
        android:id="@+id/botaoAtualizarPerfilProfissional"
        android:layout_width="0dp"
        android:layout_height="52dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="30dp"
        android:background="#AF0D25"
        android:text="Atualizar Perfil"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lblEmail" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/barra_superior2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#AF0D25"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/imagemProfissional"
        android:layout_width="101dp"
        android:layout_height="74dp"
        android:layout_marginTop="15dp"
        android:src="@drawable/ic_action_name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/barra_superior2" />

    <TextView
        android:id="@+id/nomeprofissional"
        android:layout_width="0dp"
        android:layout_height="65dp"
        android:layout_centerVertical="true"
        android:layout_marginStart="54dp"
        android:layout_marginTop="22dp"
        android:layout_marginEnd="54dp"
        android:layout_marginBottom="24dp"
        android:layout_toEndOf="@+id/image"
        android:text="@string/nome_do_profissional"
        android:textAlignment="center"
        android:textColor="#000"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/decricaoprofissional"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/image" />

    <EditText
        android:id="@+id/decricaoprofissional"
        android:layout_width="399dp"
        android:layout_height="226dp"
        android:background="#80CACACA"
        android:hint="Descrição do profissional"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lblTelefone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:text="Telefone:"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/telefoneProfissionalHome"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/emailProfissionalHome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginTop="9dp"
        android:text="email@email.com"
        android:textColor="#DD000000"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/cpfProfissionalHome"
        app:layout_constraintTop_toBottomOf="@+id/cpfProfissionalHome" />

    <TextView
        android:id="@+id/lblCPF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:text="CPF:"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/cpfProfissionalHome"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/cpfProfissionalHome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:layout_marginTop="16dp"
        android:text="000.000.000-00"
        android:textColor="#DD000000"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/lblCPF"
        app:layout_constraintTop_toBottomOf="@+id/lblEndereco" />

    <TextView
        android:id="@+id/enderecoProfissional"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="9dp"
        android:layout_marginTop="12dp"
        android:text="Rua exemplo"
        android:textColor="#DD000000"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/telefoneProfissionalHome"
        app:layout_constraintTop_toBottomOf="@+id/telefoneProfissionalHome" />

    <TextView
        android:id="@+id/telefoneProfissionalHome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="19dp"
        android:text="(00) 90000-0000"
        android:textColor="#DD000000"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/lblTelefone"
        app:layout_constraintTop_toBottomOf="@+id/decricaoprofissional" />

    <TextView
        android:id="@+id/lblEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email:"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/emailProfissionalHome"
        app:layout_constraintEnd_toStartOf="@+id/botaoAtualizarPerfilProfissional"
        app:layout_constraintStart_toStartOf="@+id/botaoAtualizarPerfilProfissional" />

    <TextView
        android:id="@+id/lblEndereco"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:text="Endereço:"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/enderecoProfissional"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/buttonSairProfissional"
        android:layout_width="100dp"
        android:layout_height="55dp"
        android:background="#00FFFFFF"
        android:text="Sair"
        style="?android:attr/borderlessButtonStyle"
        android:textColor="#FFFFFF"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="1dp" />

    <ImageView
        android:id="@+id/image4"
        android:layout_width="60dp"
        android:layout_height="60dp"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="58dp" />

</androidx.constraintlayout.widget.ConstraintLayout>